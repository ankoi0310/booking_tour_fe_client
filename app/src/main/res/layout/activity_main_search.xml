<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".search.MainSearch">

    <fragment
        android:id="@+id/header_title"
        class="vn.edu.hcmuaf.fit.travie.search.FragmentHeadSearch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:layout="@layout/fragment_head_search">
    </fragment>

    <fragment
        android:id="@+id/body_search"
        class="vn.edu.hcmuaf.fit.travie.search.FragmentBodySearch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        tools:layout="@layout/fragment_body_search">
    </fragment>

    <fragment
        android:id="@+id/button_search"
        class="vn.edu.hcmuaf.fit.travie.search.FragmentButtonSearch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="315dp"
        tools:layout="@layout/fragment_button_search">
    </fragment>

</FrameLayout>